#include<stdio.h>
#include<string.h>
void encrypt(char pass[],int key)
{ 
 	 	unsigned int i;
 	 	for(i=0;i,strlen(pass);i++)
 	 	{
 	 		pass[i] = pass[i] - key\
 	 		
 	 	}
 }
 
 int main()
 { char pass[20];
 printf("Enter the string");
 scanf("%s",pass);
 printf("Pass = %s",pass);
 encrypt(pass,0xFACA);
 printf("Encrypted value = %s",pass);
 return 0;
 }	





    
