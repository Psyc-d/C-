#include<stdio.h>
int main()
{	
	int El,Dh,C,Dr,Ce,Sum,Percent;
	printf("Enter The Marks For Electromagnetics out of 100");
  	scanf("%d",&El);
  	printf("Enter The Marks For Design History out of 100");
  	scanf("%d",&Dh);
  	printf("Enter The Marks For Calculus out of 100");
  	scanf("%d",&C);
  	printf("Enter The Marks For Design Realization out of 100");
  	scanf("%d",&Dr);
  	printf("Enter The Marks For Computational Engg out of 100");
  	scanf("%d",&Ce);
  	Sum=El+Dh+Ce+Dr+C;
  	Percent=(sum/500)*100;
  	if(Percent>90)
  	{ printf("The student got an S grade")
  	}
  	  else
  	 { if(Percent>80)
  	   { printf("The student got an A grade");
  	   }
  	   else
  	   { if(Percent>70)
  	   { printf("The student got an B grade");
  	   }
  	    else
  	    { if(Percent>60)
  	    { printf("The student got an C grade");
  	    }
  	    else
  	     { if(Percent>50)
  	     { printf("The student got an D grade");
  	     }
  	     else
  	     { if(Percent<50)
  	     { printf("The student has failed")  
  		  }
  	     }
  	     }
  	     }
  	     }
  	     }
  	     
  	
  	
